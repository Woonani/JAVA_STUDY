package kr.or.kosa;

import java.util.*;

public class Book {
	final String bookNo;
	String bookTitle;
	int price;
	
	
	public Book(String bookNo, String bookTitle, int price) {
		super();
		this.bookNo = bookNo;
		this.bookTitle = bookTitle;
		this.price = price;
	}

	public String getBookNo() {
		return bookNo;
	}

	public String getBookTitle() {
		return bookTitle;
	}


	public int getPrice() {
		return price;
	}
	
	

}


