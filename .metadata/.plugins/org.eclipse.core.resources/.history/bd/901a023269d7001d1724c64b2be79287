import java.io.File;

public class Ex09_File_Dir_Format {

	public static void main(String[] args) {
		File dir = new File("C:\\Temp");
		File[] files = dir.listFiles();
		
		for(int i =0; i < files.length; i++) {
			File file = files[i];
			
			String name = file.getName(); // 폴더명 or 파일명
			
		}
	
	}

}
