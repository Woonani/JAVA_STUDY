// 설계도를 만들때
class MyGeneric<T>{ // Type parameter
	T obj;
	
	void add(T obj) {
		this.obj = obj;
	}
	T get() {
		return this.obj;
	}
} // 타입을 잡아줄 수 있다

class Person{
	int age = 10;
}

public class Ex04_Generic {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
