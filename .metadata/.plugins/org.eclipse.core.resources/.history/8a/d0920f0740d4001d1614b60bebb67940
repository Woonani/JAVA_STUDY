import java.util.*;
public class Ex12_Map_Quiz {

	public static void main(String[] args) {
		/*
		 Map 사용하는 데이터 집합
		 지역번호, 우편번호, 회원가입(id, pwd)
		 */
		HashMap loginmap = new HashMap(); // 메모리에는 회원id,pwd저장
		loginmap.put("kim", "kim1004");
		loginmap.put("scott", "tiger");
		loginmap.put("lee", "kim1004");

	}

}
