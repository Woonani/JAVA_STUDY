		messageQueue.offer(new Message("sendKakaotalk","È«±æ³²"));
		
		while(!messageQueue.isEmpty()){
			Message message = messageQueue.poll();
			
			switch(message.command){
				
				case "sendMail" :
					System.out.println(message.to + "´Ô¿¡°Ô ¸ÞÀÏÀ» º¸³À´Ï´Ù.");	
					break;
				
